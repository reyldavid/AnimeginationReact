{
  "presets": [["env", {
    "debug": false,
    "modules": false
  }], "stage-0", "react"],
  "plugins": [
    [
        "@babel/plugin-transform-react-jsx"
    ],
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-decorators-legacy"
  ],
  "env": {
    "test": {
      "presets": [["env", {
        "modules": "commonjs"
      }]]
    }
  }
}